{
  "name": "passlint",
  "version": "0.5.1",
  "description": "Lint only serious errors that will definitely break your code, probably.",
  "main": "passlint.js",
  "bin": {
    "passlint": "./bin/cli.js"
  },
  "files": [
    "help.txt",
    "passlint.js",
    "bin/cli.js"
  ],
  "scripts": {
    "lint": "spacestandard --fix test/test.js passlint.js bin/cli.js",
    "prepublishOnly": "npm run lint && npm test",
    "test": "node test/test.js | faucet"
  },
  "keywords": [
    "passlint",
    "syntax-error",
    "acorn",
    "ecmaVersion 6 ( 2015 )"
  ],
  "author": "talmobi <talmo.christian@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/talmobi/passlint"
  },
  "homepage": "https://github.com/talmobi/passlint",
  "bugs": {
    "url": "https://github.com/talmobi/passlint/issues",
    "email": "talmo.christian@gmail.com"
  },
  "dependencies": {
    "@babel/core": "~7.11.1",
    "acorn": "~7.4.0",
    "csslint": "~1.0.5",
    "minimist": "~1.2.5",
    "redstar": "0.0.2"
  },
  "devDependencies": {
    "@babel/preset-env": "~7.11.0",
    "@talmobi/faucet": "0.0.3",
    "babelify": "~10.0.0",
    "browserify": "~16.5.2",
    "nyc": "~15.1.0",
    "spacestandard": "~0.3.0",
    "tape": "~5.0.1"
  }
}
